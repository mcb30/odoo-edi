<?xml version="1.0"?>
<odoo>
  <data>
    <record id="sale_forward_JSON_local_gateway" model="edi.gateway">
      <field name="name">Sale Forward JSON Local Gateway</field>
      <field name="model_id" ref="edi.model_edi_connection_local"/>
      <field name="username"></field>
      <field name="config_password"></field>
      <field name="server"></field>
      <field name="safety"></field>
      <field name="timeout" eval="30.0"/>
    </record>
    <record id="sale_forward_json_local_path" model="edi.gateway.path">
      <field name="name">Sale Forward JSON Local Path</field>
      <field name="sequence" eval="10"/>
      <field name="gateway_id" ref="sale_forward_JSON_local_gateway"/>
      <field name="path">/</field>
      <field name="allow_receive" eval="False"/>
      <field name="allow_send" eval="True"/>
      <field name="age_window" eval="24"/>
      <field name="doc_type_ids" eval="[(6, False, [ref('sale_forward_document_type')])]"/>
    </record>
  </data>
</odoo>
